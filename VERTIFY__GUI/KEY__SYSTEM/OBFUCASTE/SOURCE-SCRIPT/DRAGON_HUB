end

function InCorrectKey()
    print("Key is Incorrect! Please Try Key aging!")
    Notification:Notify(
    {Title = "KEY SYSTEM", Description = "Key is Incorrect! Please Try Key aging!"},
    {OutlineColor = Color3.fromRGB(255, 5, 5),Time = 5, Type = "default"}
)
end

